<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:myscroll="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_five"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">


    <com.meterial.demo.CoordinatorLayout.third.MySelectView
        android:id="@+id/my_selectview"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="50dp"
        myscroll:lineColor="@color/font_text"
        myscroll:textColor="@color/strong"
        myscroll:textSize="20dp"></com.meterial.demo.CoordinatorLayout.third.MySelectView>

    <TextView
        android:id="@+id/number_txt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginTop="20dp"
        android:text="选择的数字为:"
        android:textSize="20dp" />

    <com.meterial.demo.CoordinatorLayout.third.MyFlowLayout
        android:id="@+id/my_flowlayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp"
        android:background="@color/bg_page">
    </com.meterial.demo.CoordinatorLayout.third.MyFlowLayout>


</LinearLayout>